SRCS_cd1 = \
main.c 

OBJS_cd1 = \
main.o 

SRC_DIR_cd1 = 
OBJS_DIR = EXE_DIR = bin/Debug/

EXE = Cafeteria_Billing_Application.exe
FC = gcc.exe
LD = g++.exe
IDIR = 
CFLAGS = -Wall -g  -J$(OBJS_DIR) $(IDIR)
LFLAGS = 
LIBS = 

VPATH = $(SRC_DIR_cd1):$(OBJS_DIR)
OBJS = $(addprefix $(OBJS_DIR), $(OBJS_cd1))

all : $(EXE)

$(EXE) : $(OBJS_cd1)
	@mkdir -p $(EXE_DIR)
	$(LD) -o $(EXE_DIR)$(EXE) $(OBJS) $(LFLAGS) $(LIBS)

$(OBJS_cd1):
	@mkdir -p $(OBJS_DIR)
	$(FC) $(CFLAGS) -c $(SRC_DIR_cd1)$(@:.o=.c) -o $(OBJS_DIR)$@

clean :
	rm -f $(OBJS_DIR)*.*
	rm -f $(EXE_DIR)$(EXE)

